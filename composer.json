{
  "name": "awalhadi/addressable",
  "description": "Laravel Eloquent address management library. Manage address more than one model",
  "type": "library",
  "keywords": [
    "laravel",
    "address",
    "eloquent",
    "geocoding",
    "location"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "A Awal Hadi",
      "email": "awalhadi5@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0|^8.1|^8.2|^8.3|^8.4",
    "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/database": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
    "jackpopp/geodistance": "^1.2.3",
    "rinvex/countries": "^8.1.1"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0",
    "phpunit/phpunit": "^10.0",
    "pestphp/pest": "^2.0",
    "pestphp/pest-plugin-laravel": "^2.0",
    "laravel/pint": "^1.0",
    "nunomaduro/collision": "^7.0",
    "mockery/mockery": "^1.0",
    "fakerphp/faker": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Awalhadi\\Addressable\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Awalhadi\\Addressable\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Awalhadi\\Addressable\\Providers\\AddressableServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "pest",
    "test:coverage": "pest --coverage",
    "format": "pint",
    "format:check": "pint --test"
  }
}
